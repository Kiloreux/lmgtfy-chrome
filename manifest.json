{
    "manifest_version": 2,
    "background" : { "scripts": ["background.js"] },
    "description": "lmgtfy copy to clipboard your lmgtfy link.",
    "icons": {
        "16": "google-16.png",
        "24": "google-24.png",
        "32": "google-32.png",
        "64": "google-64.png"
    },
    "minimum_chrome_version": "6",
    "name": "Let me google that for you",
    "permissions": [ "contextMenus", "tabs", "http://*/*", "https://*/*" ],
    "version": "0.0.1"
}
